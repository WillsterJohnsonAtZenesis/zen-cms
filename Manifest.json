{
  "$schema": "https://qooxdoo.org/schema/Manifest-2-0-0.json",
  "info": {
    "name": "zx.cms",
    "summary": "Zen CMS",
    "description": "Zen [and the art of] CMS",
    "homepage": "https://zenesis.com",
    "license": "Proprietary",
    "authors": [
      {
        "name": "John Spackman (johnspackman)",
        "email": "john.spackman@zenesis.com"
      }
    ],
    "version": "0.2.1"
  },
  "provides": {
    "namespace": "zx",
    "encoding": "utf-8",
    "class": "source/class",
    "resource": "source/resource",
    "translation": "source/translation",
    "fonts": {
      "FontAwesomeBrands": {
        "defaultSize": 32,
        "comparisonString": "\\uf26e\\uf368",
        "fontFaces": [
          {
            "paths": ["zx/cms/fontawesome/fa-brands-400.eot", "zx/cms/fontawesome/fa-brands-400.woff2", "zx/cms/fontawesome/fa-brands-400.woff", "zx/cms/fontawesome/fa-brands-400.ttf"]
          }
        ],
        "glyphs": "zx/cms/fontawesome/fa-brands.json"
      },
      "FontAwesome": {
        "defaultSize": 32,
        "comparisonString": "\\uf2b9\\uf2bb",
        "fontFaces": [
          {
            "paths": ["zx/cms/fontawesome/fa-regular-400.eot", "zx/cms/fontawesome/fa-regular-400.woff2", "zx/cms/fontawesome/fa-regular-400.woff", "zx/cms/fontawesome/fa-regular-400.ttf"]
          }
        ],
        "glyphs": "zx/cms/fontawesome/fa-regular.json"
      },
      "FontAwesomeSolid": {
        "defaultSize": 32,
        "comparisonString": "\\uf2b9\\uf2bb",
        "fontFaces": [
          {
            "paths": ["zx/cms/fontawesome/fa-solid-900.eot", "zx/cms/fontawesome/fa-solid-900.woff2", "zx/cms/fontawesome/fa-solid-900.woff", "zx/cms/fontawesome/fa-solid-900.ttf"]
          }
        ],
        "glyphs": "zx/cms/fontawesome/fa-solid.json"
      }
    }
  },
  "requires": {
    "@qooxdoo/framework": "^7.0.0-beta"
  }
}
